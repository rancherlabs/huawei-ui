<div class="catalog left-nav col-md-2 col-sm-2">
  <h3 class="r-mt10 r-mb15">Catalog</h3>
  <li class="nav nav-list">
  </li>
</div>
<section>
  <div class="col-md-10 col-sm-10">
    <div class="form clearfix r-mb10">
      <div class="btn-group searchable col-md-12">
        <i class="fa fa-search"></i>
        {{input placeholder='search' class='form-control r-pl25' value=search}}
      </div>
    </div>
    <div class="col-md-12 r-pl15 r-pr15">
      {{!#each arrangedContent as |catalogItem|}}
        <div class="container-catalog text-center">
          <img src= alt= class="center-block r-mt20" style="height:75px;">
          <div class="r-mt15"></div>
          <div class="r-mt10 description"></div>
          <button class="btn btn-sm btn-primary" ><i class="fa fa-upload"></i> Launch</button>
        </div>
      {{!/each}}
    </div>
  </div>
</section>
